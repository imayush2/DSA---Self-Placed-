class Solution{
public:
    long long int factorial(int N){
        //code here
        if(N==0) return 1;
        
        return N*factorial(N-1);
    }
};




class Solution{
public:
    long long int factorial(int N){
        //code here
        long long int  res = 1;
        for(int i =2;i<=N;i++){
            res= res*i;
        }
        return res;
    }
};


